<div class="front" class="form-group">
    <h1> <u> Login</u></h1>
    <%= form_tag "/login", method: :post do %>
      <div>
      <font size=5px>  Email: <input type="email" name="email" class="form-control"> </font>
      </div>
      <div>
        Password: <input type="password" name="password" class="form-control">
      </div>
      <input type="submit" value="Log In">
    <% end %>
</div>



